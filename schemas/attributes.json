{
    "title": "DFTemplate Attributes",
    "description": "Attributes table for DF Template.",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "attribute": {
                "type": "string",
                "description": "The attribute name which triggers autocompletion."
            },
            "values": {
                "description": "All possible values for the attribute."
            }
        },
        "required": [
            "attribute",
            "values"
        ]
    },
    "default": {}
}