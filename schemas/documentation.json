{
    "title": "DFTemplate Documentation",
    "description": "Documentation table for DF Template.",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": {
        "type": "object",
        "description": "An element",
        "properties": {
            "category": {
                "type": "string",
                "description": "The category of the element."
            },
            "signature": {
                "type": "string",
                "description": "DFTemplate code signatue of the element."
            },
            "summary": {
                "type": "string",
                "description": "A short description of the element."
            },
            "parameters": {
                "type": "array",
                "description": "Parameters for a symbol definition.",
                "items": {
                    "properties": {
                        "name": {
                            "type": "string",
                            "description": "The name of the parameter."
                        },
                        "description": {
                            "type": "string",
                            "description": "A short description of the parameter."
                        }
                    },
                    "required": [
                        "name",
                        "description"
                    ]
                }
            }
        },
        "required": [
            "summary"
        ]
    },
    "default": {}
}