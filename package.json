{
    "name": "dftemplate",
    "displayName": "DFTemplate",
    "description": "DF TEMPLATE 1.11 for Visual Studio Code",
    "icon": "images/icon.png",
    "version": "0.1.0",
    "publisher": "thelacus",
    "author": {
        "name": "TheLacus"
    },
    "contributors": [
        {
            "name": "TheLacus",
            "url": "https://github.com/TheLacus"
        }
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/TheLacus/vscode-dftemplate"
    },
    "bugs": {
        "url": "https://github.com/TheLacus/vscode-dftemplate/issues"
    },
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:dftemplate"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "dftemplate",
                "aliases": [
                    "DfTemplate",
                    "dftemplate"
                ],
                "extensions": [
                    ".dftemplate"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dftemplate",
                "scopeName": "source.dftemplate",
                "path": "./syntaxes/dftemplate.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "dftemplate",
                "path": "./snippets/dftemplate.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}